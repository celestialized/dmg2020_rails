


<div class="row">
  <div class="col-sm-6">
    <h1>Source_code_credits</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_source_code_credit_path, class: 'btn btn-primary' do %>
    Add New Source code credit
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    

            <th>Name of work</th>
            <th>Url</th>
            <th>License kind</th>
            <th>License text</th>
            
          <th></th>
        
      </tr>
    </thead>

    <tbody>
      <% @source_code_credits.each do |source_code_credit| %>
        <%= content_tag :tr, id: dom_id(source_code_credit), class: dom_class(source_code_credit) do %>
          

                      <td><%= source_code_credit.name_of_work %></td>
                      <td><%= source_code_credit.url %></td>
                      <td><%= source_code_credit.license_kind %></td>
                      <td><%= source_code_credit.license_text %></td>
          
          
            <td><%= link_to 'Show', source_code_credit %></td>
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
